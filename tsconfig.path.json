    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components", "components/*"],
      "@nativeComponents/*": ["native components", "native components/*"],
      "@redux/*": ["redux", "redux/*"],
      "@utils/*": ["utils", "utils/*"],
